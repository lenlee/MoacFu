page{ background: #f5f5f5;}
button{ margin: 0;}
text{ font-size: 30rpx; color: #000;}
input{ line-height: normal;}
.placeholder{ font-size: 30rpx;}
.tab{float: left; width: 50%;text-align: center;padding: 25rpx 0; color: #BFBFBF; }
.topTabSwiper{border-top: 1px solid #f5f5f5; background: #ffffff; zoom: 1;}
.topTabSwiper:after{content: ""; clear: both;display: block;}
.tabBorer{border-bottom: 1px solid #E64340; color: #E64340;}
.swiper{width: 100%;}
.swiper_con{text-align: center; width: 100%; height: 100%; padding: 30rpx 0; }
.section{ padding: 0 20rpx;}
.weui-cells{border:1px solid #f5f5f5;}
.weui-cells_after-title{ background: #ffffff; border-radius: 5rpx;}
.weui-label{ text-align: left;}
.weui-cell{ position: static; padding: 15rpx 0; margin: 0 25rpx;}
.weui-cell-bottom{ border-bottom: 1px solid #EFEFEF;}
.weui-input-words{ margin-top: 30rpx; padding: 15rpx 0; background: #ffffff; border-radius: 5rpx;}
.weui-input{ font-size: 30rpx;}